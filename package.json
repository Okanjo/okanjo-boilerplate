{
  "private": true,
  "name": "okanjo-boilerplate",
  "version": "6.0.0",
  "description": "Okanjo Boilerplate",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf .nyc_output coverage ui/static/dist/*",
    "build": "node_modules/.bin/webpack",
    "test": "node_modules/.bin/mocha -R spec test",
    "cover": "node_modules/.bin/nyc node_modules/mocha/bin/_mocha --exit",
    "lint": "node_modules/.bin/eslint .",
    "report": "npm run cover && npm run lint",
    "watch": "npm run build --watch",
    "docker": "docker run -it -v /app/node_modules -v $(pwd):/app -w /app node:18 /bin/bash",
    "start": "node . web"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/okanjo/okanjo-boilerplate.git"
  },
  "keywords": [
    "Okanjo",
    "Boilerplate",
    "React",
    "Redux",
    "Bootstrap"
  ],
  "author": "Okanjo Partners Inc",
  "license": "MIT",
  "dependencies": {
    "@hapi/catbox-redis": "^7.0.0",
    "@hapi/good": "^9.0.1",
    "@hapi/good-console": "^9.0.1",
    "@hapi/good-squeeze": "^6.0.0",
    "base-id": "^3.2.0",
    "commander": "^9.4.1",
    "debug": "^4.3.4",
    "hapi-auth-basic-key": "^6.0.0",
    "joi": "^17.7.0",
    "okanjo-app": "^3.2.1",
    "okanjo-app-broker": "^3.2.0",
    "okanjo-app-redis": "^4.0.1",
    "okanjo-app-server": "^3.0.0",
    "okanjo-app-server-session": "^3.1.0",
    "nanoid": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.19.4",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime-corejs3": "^7.20.1",
    "@hapi/catbox-memory": "^6.0.0",
    "@redux-devtools/core": "^3.13.1",
    "@redux-devtools/dock-monitor": "^3.0.1",
    "@redux-devtools/log-monitor": "^4.0.1",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.0.1",
    "bootstrap": "^5.2.2",
    "chokidar": "^3.5.3",
    "classnames": "^2.3.2",
    "clean-css": "^5.3.1",
    "core-js": "^3.26.0",
    "css-loader": "^6.7.1",
    "es6-promise": "^4.2.8",
    "eslint": "^8.26.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.10",
    "history": "^5.3.0",
    "html5shiv": "^3.7.3",
    "mini-css-extract-plugin": "^2.6.1",
    "mocha": "^10.1.0",
    "moment": "^2.29.4",
    "needle": "^3.1.0",
    "normalize.css": "^8.0.1",
    "nyc": "^15.1.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.4",
    "react-router": "^6.4.3",
    "react-router-bootstrap": "^0.26.2",
    "react-router-dom": "^6.4.3",
    "react-router-redux": "^4.0.8",
    "redux": "^4.2.0",
    "redux-localstorage": "^0.4.1",
    "redux-thunk": "^2.4.1",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "should": "^13.2.3",
    "style-loader": "^3.3.1",
    "title-case": "^3.0.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "whatwg-fetch": "^3.6.2"
  },
  "mocha": {
    "require": [
      "should",
      "./test/env.js"
    ],
    "reporter": "spec",
    "checkLeaks": true,
    "recursive": true
  },
  "nyc": {
    "reporter": [
      "text-summary",
      "html",
      "lcovonly"
    ]
  },
  "eslintConfig": {
    "ignorePatterns": [
      "/node_modules/**",
      "/ui/static/dist/**",
      "/.nyc_output/**",
      "/coverage/**"
    ],
    "parser": "@babel/eslint-parser",
    "env": {
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
      },
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "rules": {
      "strict": 0,
      "semi": 1,
      "no-console": 1
    },
    "globals": {
      "require": true,
      "module": true,
      "describe": true,
      "it": true,
      "before": true,
      "beforeEach": true,
      "after": true,
      "afterEach": true,
      "Promise": true,
      "window": true,
      "document": true
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry",
          "corejs": "3.0.0"
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "browserslist": [
    "> 5%"
  ]
}
